<?xml version="1.0"?>
<config>
    <modules>
        <Anowave_BulkDelete>
            <version>0.0.0.1</version>
        </Anowave_BulkDelete>
    </modules>
    <global>
    	<models>
		    <bulkdelete>
		        <class>Anowave_BulkDelete_Model</class>
		    </bulkdelete>
		</models>
        <helpers>
	        <bulkdelete>
	            <class>Anowave_BulkDelete_Helper</class>
	        </bulkdelete>
	    </helpers>
	    <blocks>
            <bulkdelete>
            	<class>Anowave_BulkDelete_Block</class>
            </bulkdelete>
            <adminhtml>
				<rewrite>
					<catalog_product_attribute_grid>Anowave_BulkDelete_Block_Grid</catalog_product_attribute_grid>
				</rewrite>
			</adminhtml>
        </blocks>
    </global>
    <admin>
		<routers>
			<adminhtml>
				<args>
					<modules>
						<anowave_bulkdelete>Anowave_BulkDelete</anowave_bulkdelete>
					</modules>
				</args>
			</adminhtml>
		</routers>
	</admin>
</config>